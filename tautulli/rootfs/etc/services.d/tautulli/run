#!/usr/bin/with-contenv bashio
# ==============================================================================
# Home Assistant Community Add-on: Tautulli
# Runs Tautulli
# ==============================================================================
bashio::log.info "Starting Tautulli..."
exec \
  python3 /opt/Tautulli.py \
    --datadir /data \
    --nolaunch \
    -p 8181